#ifndef CPPMENU
#define CPPMENU

class longint {
    public:
        void set_num(long num)
        {
            name = num;
        }
        
        int calc_longint(int num)
        {
            while(num!=0)
            {
                temp=num%10;
                num=num/10;
                counted=counted+temp;
            }
            return counted;
        }
    private:
        int counted;
        int temp;
};

#endif // CPPMENU

